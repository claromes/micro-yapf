install:
	mkdir -p ~/.config/micro/plug/yapf/help
	cp help/yapf.md ~/.config/micro/plug/yapf/help/yapf.md
	cp yapf.lua ~/.config/micro/plug/yapf/yapf.lua
	cp repo.json ~/.config/micro/plug/yapf/repo.json
	cp LICENSE ~/.config/micro/plug/yapf/LICENSE
	cp README.md ~/.config/micro/plug/yapf/README.md
